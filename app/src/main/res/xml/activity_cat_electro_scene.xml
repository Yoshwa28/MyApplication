<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="258dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginRight="15dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.578"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="373dp"
            android:id="@+id/imagen0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintVertical_bias="0.026"
            android:layout_height="0dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginRight="15dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.627"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:id="@+id/imagen0" />
        <Constraint
            android:layout_height="667dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="406dp"
            motion:layout_constraintTop_toBottomOf="@+id/imagen0"
            android:id="@+id/linearLayout"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteY="-32dp" />
    </ConstraintSet>
</MotionScene>